    <string-array name="voice_input_modes">
        <item>På hovedtastatur</item>
        <item>På talltastatur</item>
        <item>Av</item>
    </string-array>
    <!-- Don't translate -->
    <!-- Don't translate -->
    <!-- Don't translate -->
